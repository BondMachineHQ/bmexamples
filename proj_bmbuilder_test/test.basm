%meta ioatt	testio1 cp: bm, index:0, type:output
%meta ioatt	testio2 cp: cpu1, index:0, type:input
%meta ioatt	testio2 cp: bm, index:0, type:input
%meta ioatt	testio3 cp: cpu1, index:1, type:output
%meta ioatt	testio3 cp: bm, index:1, type:output
