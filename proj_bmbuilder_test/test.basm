%section code1 .romtext iomode:async
        entry _start    ; Entry point
_start:
	mov	r0, i0
	mov	r1, i1
	inc	r0
	mov	o0, r0
	mov	o1, r0
	j 	_start

%endsection

%meta cpdef	cpu1	romcode: code1, ramsize:8
%meta bmdef	global  registersize:32
