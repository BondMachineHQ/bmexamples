
#
# General
#
CONFIG_WORKING_DIR="working_dir"
CONFIG_GENERAL_TYPE_BOARD=y
# CONFIG_GENERAL_TYPE_BOARDLESS is not set
CONFIG_MAPFILE="default_maps.json"
# end of General

#
# BondMachine configuration
#
# CONFIG_PROJECT_TYPE_BONDGO is not set
# CONFIG_PROJECT_TYPE_NEURALBOND is not set
# CONFIG_PROJECT_TYPE_MELBOND is not set
CONFIG_PROJECT_TYPE_BASM=y
CONFIG_SOURCE_BASM="counter.basm"
CONFIG_BASM_DEBUG=y
# end of BondMachine configuration

#
# Board and toolchain
#
CONFIG_TOOLCHAIN_XILINX=y
# CONFIG_TOOLCHAIN_ALTERA is not set
# CONFIG_TOOLCHAIN_LATTICE is not set
# CONFIG_XILINX_BOARD_ZEDBOARD is not set
CONFIG_XILINX_BOARD_BASYS3=y
# CONFIG_XILINX_BOARD_ZC702 is not set
# CONFIG_XILINX_BOARD_KC705 is not set
# CONFIG_XILINX_BOARD_EBAZ4205 is not set
# end of Board and toolchain

#
# External modules
#
# CONFIG_UART_SUPPORT is not set
CONFIG_BOARD_SLOW=y
CONFIG_BOARD_SLOW_FACTOR=23
# end of External modules

#
# Testing and debugging
#
# CONFIG_REGRESSION_TEST is not set
# end of Testing and debugging
